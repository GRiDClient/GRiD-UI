info {
	name: Maya Theme
	creator: Stumblinbear
}

vars {
	maincolor = #FF1919
}

global {
	color: #FFF
}

panel {
	background-color: #FFF
	opacity: 80%
	border-right: inner 5px #777
}

window {
	background-color: #FFF
	opacity: 90%
	border-bottom: inner 5px #777
}

window .title {
	height: 25px
	
	background-color: [maincolor]
	border-bottom: inner 5px #FF3B3B
}